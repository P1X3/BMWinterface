<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.osovskiy.bmwinterface.dummy" >

    <uses-permission android:name="com.osovskiy.bmwinterface.permission.RECEIVE_MSG_PERMISSION" />
    <uses-permission android:name="com.osovskiy.bmwinterface.permission.SEND_MSG_PERMISSION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <receiver android:name=".NewMsgReceiver" >
            <intent-filter>
                <action android:name="com.osovskiy.bmwinterface.ACTION_NEW_BUS_MESSAGE" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".MyActivity"
            android:label="@string/title_activity_my" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

      <meta-data android:name="plugin_author" android:value="Vadim Osovskiy" />
    </application>

</manifest>
